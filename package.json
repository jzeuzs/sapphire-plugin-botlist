{
	"name": "@devtomio/plugin-botlist",
	"version": "1.3.0",
	"description": "Plugin for @sapphire/framework to post stats to several discord bot lists.",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"module": "dist/index.mjs",
	"typedocMain": "src/index.ts",
	"author": "Tomio <https://github.com/devtomio>",
	"license": "MIT",
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		},
		"./register": {
			"import": "./register.mjs",
			"require": "./register.js"
		}
	},
	"sideEffects": [
		"./dist/register.js",
		"./register.js",
		"./register.mjs"
	],
	"scripts": {
		"lint": "eslint src --ext ts --fix",
		"format": "prettier --write src/**/*.ts README.md package.json scripts/**/*",
		"docs": "typedoc",
		"update": "yarn upgrade-interactive",
		"build": "tsc -b src && yarn esm:register && yarn esm:default",
		"esm:register": "gen-esm-wrapper dist/register.js dist/register.mjs",
		"esm:default": "gen-esm-wrapper dist/index.js dist/index.mjs",
		"clean": "tsc -b src --clean",
		"watch": "tsc -b src -w",
		"bump": "cliff-jumper",
		"check-update": "cliff-jumper --dry-run",
		"check-botlists": "node scripts/check.mjs",
		"sversion": "standard-version",
		"commit": "git-cz",
		"cz": "git-cz",
		"postinstall": "husky install .github/husky",
		"prepack": "yarn build && pinst --disable",
		"postpack": "pinst --enable"
	},
	"dependencies": {
		"tiny-typed-emitter": "^2.1.0",
		"tslib": "^2.8.0",
		"undici": "^5.28.4"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.8.1",
		"@commitlint/config-conventional": "^17.8.1",
		"@favware/cliff-jumper": "^1.10.0",
		"@favware/npm-deprecate": "^1.0.7",
		"@sapphire/eslint-config": "^4.4.3",
		"@sapphire/framework": "^4.8.5",
		"@sapphire/prettier-config": "^1.4.5",
		"@sapphire/ts-config": "^3.3.4",
		"@types/node": "^18.19.57",
		"@typescript-eslint/eslint-plugin": "^5.49.0",
		"@typescript-eslint/parser": "^5.49.0",
		"colorette": "^2.0.20",
		"cz-conventional-changelog": "^3.3.0",
		"discord.js": "^14.16.3",
		"eslint": "^8.57.1",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-prettier": "^4.2.1",
		"gen-esm-wrapper": "^1.1.3",
		"husky": "^8.0.3",
		"lint-staged": "^13.3.0",
		"pinst": "^3.0.0",
		"prettier": "^2.8.8",
		"pretty-quick": "^3.3.1",
		"standard-version": "^9.5.0",
		"typedoc": "^0.23.24",
		"typedoc-plugin-mdn-links": "^2.0.2",
		"typescript": "^4.9.4"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/devtomio/sapphire-plugin-botlist.git"
	},
	"files": [
		"dist",
		"!dist/*.tsbuildinfo",
		"register.*"
	],
	"engines": {
		"node": ">=v16.6.0",
		"npm": ">=7.0.0"
	},
	"keywords": [
		"sapphiredev",
		"bot",
		"typescript",
		"ts",
		"yarn",
		"discord",
		"sapphire",
		"discordjs",
		"botlist",
		"topgg"
	],
	"bugs": {
		"url": "https://github.com/devtomio/sapphire-plugin-botlist/issues"
	},
	"homepage": "https://devtomio.github.io/sapphire-plugin-botlist",
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"publishConfig": {
		"access": "public"
	},
	"prettier": "@sapphire/prettier-config",
	"packageManager": "yarn@3.8.6"
}
