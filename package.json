{
	"name": "@devtomio/plugin-botlist",
	"version": "1.2.0",
	"description": "Plugin for @sapphire/framework to post stats to several discord bot lists.",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"module": "dist/index.mjs",
	"typedocMain": "src/index.ts",
	"author": "Tomio <mail@tomio.codes>",
	"license": "MIT",
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		},
		"./register": {
			"import": "./register.mjs",
			"require": "./register.js"
		}
	},
	"sideEffects": [
		"./dist/register.js",
		"./register.js",
		"./register.mjs"
	],
	"scripts": {
		"lint": "eslint src --ext ts --fix",
		"format": "prettier --write src/**/*.ts",
		"docs": "typedoc",
		"test": "jest",
		"test:watch": "jest --watch",
		"update": "yarn upgrade-interactive",
		"build": "tsc -b src && yarn esm:register && yarn esm:default",
		"esm:register": "gen-esm-wrapper dist/register.js dist/register.mjs",
		"esm:default": "gen-esm-wrapper dist/index.js dist/index.mjs",
		"clean": "tsc -b src --clean",
		"watch": "tsc -b src -w",
		"sversion": "standard-version",
		"commit": "git-cz",
		"cz": "git-cz",
		"prepublishOnly": "yarn build",
		"prepare": "husky install .github/husky"
	},
	"dependencies": {
		"@sapphire/fetch": "^2.4.1",
		"tiny-typed-emitter": "^2.1.0",
		"tslib": "^2.4.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^16.3.0",
		"@commitlint/config-conventional": "^16.2.4",
		"@sapphire/eslint-config": "^4.3.7",
		"@sapphire/framework": "^2.5.1",
		"@sapphire/prettier-config": "^1.4.3",
		"@sapphire/ts-config": "^3.3.4",
		"@types/node": "^17.0.8",
		"@typescript-eslint/eslint-plugin": "^5.33.1",
		"@typescript-eslint/parser": "^5.33.1",
		"cz-conventional-changelog": "^3.3.0",
		"discord.js": "^13.10.2",
		"eslint": "^8.22.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.2.1",
		"gen-esm-wrapper": "^1.1.3",
		"husky": "^7.0.4",
		"lint-staged": "^12.5.0",
		"prettier": "^2.7.1",
		"pretty-quick": "^3.1.3",
		"standard-version": "^9.3.2",
		"typedoc": "^0.23.10",
		"typedoc-plugin-mdn-links": "^2.0.0",
		"typescript": "^4.7.4"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/devtomio/sapphire-plugin-botlist.git"
	},
	"files": [
		"dist",
		"!dist/*.tsbuildinfo",
		"register.*"
	],
	"engines": {
		"node": ">=v16.6.0",
		"npm": ">=7.0.0"
	},
	"keywords": [
		"sapphiredev",
		"bot",
		"typescript",
		"ts",
		"yarn",
		"discord",
		"sapphire",
		"discordjs",
		"botlist",
		"topgg"
	],
	"bugs": {
		"url": "https://github.com/devtomio/sapphire-plugin-botlist/issues"
	},
	"homepage": "https://devtomio.github.io/sapphire-plugin-botlist",
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"publishConfig": {
		"access": "public"
	},
	"prettier": "@sapphire/prettier-config",
	"packageManager": "yarn@3.2.2"
}
